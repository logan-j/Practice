    pass

options = {"-h" : helper, "-r" : fib_rec, "-i" : fib_iter}

arg = sys.argv

number = -1

for item in sys.argv:
    try:
        number = int(item)
    except ValueError:
        pass

if number > 0:
    

fibby = options["-h"]

