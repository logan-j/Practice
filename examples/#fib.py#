#fibonacci sequence

#assumes there is no 0th fibonacci number. (1, 1, 2, 3, etc.)

#imported for command line options
import sys


#iterative generator function for fibonacci numbers
def fib_iter(n = 0):
    a, b = 1, 1
    while True:
        yield a
        a, b = b, a+b

#generic recursive function
def fib_rec(n = 0):
    
if n == 1 or n == 2:
        return 1
    else:
        return fib_rec(n - 1) + fib_rec(n - 2)


